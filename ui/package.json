  "lint-staged": {
    "linters": {
      "src/**/*.{ts,tsx,json,css}": [
        "prettier --single-quote --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "format": "prettier --single-quote --write 'src/**/*.@(ts|tsx)'",
    "precommit": "lint-staged",
    "start": "node scripts/start.js",
    "build": "yarn generateVersion && node scripts/build.js",
    "generateVersion": "node commands/generateVersion.js",
    "compress": "gzip -5 --keep -r build/static/js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "tslint 'src/**/*.@(ts|tsx)'",
    "componentTest": "jest --runInBand --forceExit --detectOpenHandles --coverage",
    "open-cypress": "yarn run cypress open",
    "record-cypress": "cypress run --record --key fd048d49-6b4c-4e6b-885f-819a533c2fc4"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "^4.9.0",
    "enzyme": "^3.3.0",
